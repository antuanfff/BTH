      29.  00:4F0F                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30.  00:4F0F                      		; --- INPUT: A -> sound to be played ---
      31.  00:4F0F                      		; ---        C -> sound priority     ---
      32.  00:4F0F  C5                  		push	bc				; Store bc in stack
      33.  00:4F10  D5                  		push	de				; Store de in stack
      34.  00:4F11  E5                  		push	hl				; Store hl in stack
      35.  00:4F12                      		; --- Check if the index is in the bank ---
      36.  00:4F12  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37.  00:4F13  2A C3 C3            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
