      17.  00:4EFE                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18.  00:4EFE                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19.  00:4EFE  22 C3 C3            		ld	[ayFX_BANK],hl			; Current ayFX bank
