      65.  00:50DC                    ~ 			add	a,a				; a:=a*4
      66.  00:50DC                    ~ 			add	a,a				; a:=a*8
      67.  00:50DC                    ~ 			add	a,a				; a:=a*16
      68.  00:50DC                    ~ 			ld	e,a				; e:=a
      69.  00:50DC                    ~ 			ld	d,0				; de:=a
