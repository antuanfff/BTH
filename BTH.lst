      52.  00:4E76  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53.  00:4E78                      		; --- Set new priority ---
      54.  00:4E78  79                  		ld	a,c				; a:=New priority
      55.  00:4E79  E6 0F               		and	$0F				; We mask the priority
      56.  00:4E7B  32 B9 C3            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57.  00:4E7E                      
      58.  00:4E7E                      		IF ( AYFXRELATIVE == 1 )
      59.  00:4E7E                    ~ 			; --- Volume adjust using PT3 volume table ---
