      61.  00:4E7E                    ~ 			ld	a,15				; a:=15
      62.  00:4E7E                    ~ 			sub	c				; a:=15-New priority = relative volume
      63.  00:4E7E                    ~ 			jr	z,@@INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64.  00:4E7E                    ~ 			add	a,a				; a:=a*2
      65.  00:4E7E                    ~ 			add	a,a				; a:=a*4
      66.  00:4E7E                    ~ 			add	a,a				; a:=a*8
      67.  00:4E7E                    ~ 			add	a,a				; a:=a*16
      68.  00:4E7E                    ~ 			ld	e,a				; e:=a
      69.  00:4E7E                    ~ 			ld	d,0				; de:=a
