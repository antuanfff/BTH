      24.  00:50B6                      ayFX_END:	; --- End of an ayFX stream ---
      25.  00:50B6  3E FF               		ld	a,255				; Lowest ayFX priority
      26.  00:50B8  32 EA C3            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27.  00:50BB  C9                  		ret					; Return
