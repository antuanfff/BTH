      17.  00:4F2B                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18.  00:4F2B                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19.  00:4F2B  22 D2 C3            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20.  00:4F2E  AF                  		xor	a				; a:=0
      21.  00:4F2F  32 D1 C3            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22.  00:4F32  3C                  		inc	a				; Starting channel (=1)
      23.  00:4F33  32 DB C3            		ld	[ayFX_CHANNEL],a		; Updated
      24.  00:4F36                      ayFX_END:	; --- End of an ayFX stream ---
      25.  00:4F36  3E FF               		ld	a,255				; Lowest ayFX priority
      26.  00:4F38  32 D4 C3            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27.  00:4F3B  C9                  		ret					; Return
      28.  00:4F3C                      
      29.  00:4F3C                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30.  00:4F3C                      		; --- INPUT: A -> sound to be played ---
      31.  00:4F3C                      		; ---        C -> sound priority     ---
      32.  00:4F3C  C5                  		push	bc				; Store bc in stack
      33.  00:4F3D  D5                  		push	de				; Store de in stack
      34.  00:4F3E  E5                  		push	hl				; Store hl in stack
      35.  00:4F3F                      		; --- Check if the index is in the bank ---
      36.  00:4F3F  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37.  00:4F40  2A D2 C3            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38.  00:4F43  7E                  		ld	a,[hl]				; Number of samples in the bank
      39.  00:4F44  B7                  		or	a				; If zero (means 256 samples)...
      40.  00:4F45  28 06               		jr	z,.CHECK_PRI			; ...goto @@CHECK_PRI
      41.  00:4F47                      		; The bank has less than 256 samples
      42.  00:4F47  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43.  00:4F48  BE                  		cp	[hl]				; If new index is not in the bank...
      44.  00:4F49  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45.  00:4F4B  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46.  00:4F4D                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47.  00:4F4D                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48.  00:4F4D  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49.  00:4F4E  3A D4 C3            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50.  00:4F51  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51.  00:4F52  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52.  00:4F54  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53.  00:4F56                      		; --- Set new priority ---
      54.  00:4F56  79                  		ld	a,c				; a:=New priority
      55.  00:4F57  E6 0F               		and	$0F				; We mask the priority
      56.  00:4F59  32 D4 C3            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57.  00:4F5C                      
      58.  00:4F5C                      		IF ( AYFXRELATIVE == 1 )
      59.  00:4F5C                    ~ 			; --- Volume adjust using PT3 volume table ---
      60.  00:4F5C                    ~ 			ld	c,a				; c:=New priority (fixed)
      61.  00:4F5C                    ~ 			ld	a,15				; a:=15
      62.  00:4F5C                    ~ 			sub	c				; a:=15-New priority = relative volume
      63.  00:4F5C                    ~ 			jr	z,@@INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64.  00:4F5C                    ~ 			add	a,a				; a:=a*2
      65.  00:4F5C                    ~ 			add	a,a				; a:=a*4
      66.  00:4F5C                    ~ 			add	a,a				; a:=a*8
      67.  00:4F5C                    ~ 			add	a,a				; a:=a*16
      68.  00:4F5C                    ~ 			ld	e,a				; e:=a
      69.  00:4F5C                    ~ 			ld	d,0				; de:=a
      70.  00:4F5C                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
