      17.  00:4DB9                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18.  00:4DB9                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19.  00:4DB9  22 98 C3            		ld	[ayFX_BANK],hl			; Current ayFX bank
