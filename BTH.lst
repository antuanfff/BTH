      29.  00:4E5E                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30.  00:4E5E                      		; --- INPUT: A -> sound to be played ---
      31.  00:4E5E                      		; ---        C -> sound priority     ---
      32.  00:4E5E  C5                  		push	bc				; Store bc in stack
      33.  00:4E5F  D5                  		push	de				; Store de in stack
      34.  00:4E60  E5                  		push	hl				; Store hl in stack
      35.  00:4E61                      		; --- Check if the index is in the bank ---
      36.  00:4E61  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37.  00:4E62  2A B7 C3            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38.  00:4E65  7E                  		ld	a,[hl]				; Number of samples in the bank
      39.  00:4E66  B7                  		or	a				; If zero (means 256 samples)...
      40.  00:4E67  28 06               		jr	z,.CHECK_PRI			; ...goto @@CHECK_PRI
      41.  00:4E69                      		; The bank has less than 256 samples
      42.  00:4E69  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43.  00:4E6A  BE                  		cp	[hl]				; If new index is not in the bank...
