      17.  00:4F3A                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18.  00:4F3A                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19.  00:4F3A  22 D2 C3            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20.  00:4F3D  AF                  		xor	a				; a:=0
      21.  00:4F3E  32 D1 C3            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22.  00:4F41  3C                  		inc	a				; Starting channel (=1)
      23.  00:4F42  32 DB C3            		ld	[ayFX_CHANNEL],a		; Updated
      24.  00:4F45                      ayFX_END:	; --- End of an ayFX stream ---
      25.  00:4F45  3E FF               		ld	a,255				; Lowest ayFX priority
      26.  00:4F47  32 D4 C3            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27.  00:4F4A  C9                  		ret					; Return
      28.  00:4F4B                      
      29.  00:4F4B                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30.  00:4F4B                      		; --- INPUT: A -> sound to be played ---
      31.  00:4F4B                      		; ---        C -> sound priority     ---
      32.  00:4F4B  C5                  		push	bc				; Store bc in stack
      33.  00:4F4C  D5                  		push	de				; Store de in stack
      34.  00:4F4D  E5                  		push	hl				; Store hl in stack
      35.  00:4F4E                      		; --- Check if the index is in the bank ---
      36.  00:4F4E  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37.  00:4F4F  2A D2 C3            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38.  00:4F52  7E                  		ld	a,[hl]				; Number of samples in the bank
      39.  00:4F53  B7                  		or	a				; If zero (means 256 samples)...
      40.  00:4F54  28 06               		jr	z,.CHECK_PRI			; ...goto @@CHECK_PRI
      41.  00:4F56                      		; The bank has less than 256 samples
      42.  00:4F56  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43.  00:4F57  BE                  		cp	[hl]				; If new index is not in the bank...
      44.  00:4F58  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45.  00:4F5A  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46.  00:4F5C                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47.  00:4F5C                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48.  00:4F5C  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49.  00:4F5D  3A D4 C3            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50.  00:4F60  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51.  00:4F61  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52.  00:4F63  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53.  00:4F65                      		; --- Set new priority ---
      54.  00:4F65  79                  		ld	a,c				; a:=New priority
      55.  00:4F66  E6 0F               		and	$0F				; We mask the priority
      56.  00:4F68  32 D4 C3            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57.  00:4F6B                      
      58.  00:4F6B                      		IF ( AYFXRELATIVE == 1 )
      59.  00:4F6B                    ~ 			; --- Volume adjust using PT3 volume table ---
      60.  00:4F6B                    ~ 			ld	c,a				; c:=New priority (fixed)
      61.  00:4F6B                    ~ 			ld	a,15				; a:=15
      62.  00:4F6B                    ~ 			sub	c				; a:=15-New priority = relative volume
      63.  00:4F6B                    ~ 			jr	z,@@INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
