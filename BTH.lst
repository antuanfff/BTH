      46.  00:43B8  57                  		ld	d,a				; D = Valor del slot primario
      47.  00:43B9  78                  		ld	a,b				; Recuperamos el slot
      48.  00:43BA  E6 03               		and	$03				; Nos fijamos en el slot primario
