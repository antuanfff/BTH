      45.  00:4E6D  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46.  00:4E6F                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47.  00:4E6F                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48.  00:4E6F  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49.  00:4E70  3A B9 C3            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50.  00:4E73  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
