      29.  00:4DCA                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30.  00:4DCA                      		; --- INPUT: A -> sound to be played ---
      31.  00:4DCA                      		; ---        C -> sound priority     ---
      32.  00:4DCA  C5                  		push	bc				; Store bc in stack
      33.  00:4DCB  D5                  		push	de				; Store de in stack
      34.  00:4DCC  E5                  		push	hl				; Store hl in stack
      35.  00:4DCD                      		; --- Check if the index is in the bank ---
      36.  00:4DCD  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37.  00:4DCE  2A 98 C3            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38.  00:4DD1  7E                  		ld	a,[hl]				; Number of samples in the bank
      39.  00:4DD2  B7                  		or	a				; If zero (means 256 samples)...
      40.  00:4DD3  28 06               		jr	z,.CHECK_PRI			; ...goto @@CHECK_PRI
      41.  00:4DD5                      		; The bank has less than 256 samples
      42.  00:4DD5  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43.  00:4DD6  BE                  		cp	[hl]				; If new index is not in the bank...
