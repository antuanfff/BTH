      29.  00:4EF4                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30.  00:4EF4                      		; --- INPUT: A -> sound to be played ---
      31.  00:4EF4                      		; ---        C -> sound priority     ---
      32.  00:4EF4  C5                  		push	bc				; Store bc in stack
      33.  00:4EF5  D5                  		push	de				; Store de in stack
      34.  00:4EF6  E5                  		push	hl				; Store hl in stack
      35.  00:4EF7                      		; --- Check if the index is in the bank ---
      36.  00:4EF7  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37.  00:4EF8  2A C3 C3            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38.  00:4EFB  7E                  		ld	a,[hl]				; Number of samples in the bank
      39.  00:4EFC  B7                  		or	a				; If zero (means 256 samples)...
      40.  00:4EFD  28 06               		jr	z,.CHECK_PRI			; ...goto @@CHECK_PRI
      41.  00:4EFF                      		; The bank has less than 256 samples
      42.  00:4EFF  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43.  00:4F00  BE                  		cp	[hl]				; If new index is not in the bank...
