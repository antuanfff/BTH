      17.  00:4EE3                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18.  00:4EE3                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19.  00:4EE3  22 C3 C3            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20.  00:4EE6  AF                  		xor	a				; a:=0
