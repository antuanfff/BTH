      45.  00:4F03  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46.  00:4F05                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47.  00:4F05                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48.  00:4F05  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49.  00:4F06  3A C5 C3            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
